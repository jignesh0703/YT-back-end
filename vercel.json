{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/video/upload",
      "dest": "src/server.js"
    }
  ],
  "functions": {
    "api/video/upload": {
      "maxDuration": 60,
      "memory": 1024
    }
  }
}